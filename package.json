{
  "name": "cupom-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/server.ts",
    "clean": "rm -rf node_modules dist",
    "prod:start": "npm run build && pm2 start ecosystem.config.js --env production",
    "prod:stop": "pm2 stop coupon-server",
    "prod:restart": "npm run build && pm2 restart coupon-server",
    "prod:logs": "pm2 logs coupon-server",
    "prod:list": "pm2 list"
  },
  "repository": "git@github.com:alefrodrigues538/coupon-server.git",
  "author": "Alef F R Santos",
  "license": "ISC",
  "keywords": [],
  "dependencies": {
    "bcrypt": "^6.0.0",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^24.0.4",
    "nodemon": "^3.1.10"
  }
}
